{
  "name": "@ranger/ts-parser",
  "version": "1.0.0",
  "description": "Fast TypeScript parser written in Ranger - cross-compilable to JS, Rust, C++, Go, and Swift",
  "type": "module",
  "main": "benchmark/ts_parser_module.cjs",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "benchmark": "node benchmark/benchmark.js",
    "benchmark:large": "node benchmark/benchmark.js --large"
