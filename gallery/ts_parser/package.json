  "keywords": [
    "typescript",
    "parser",
    "ast",
    "jsx",
    "tsx",
    "ranger"
  ],
  "devDependencies": {
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "typescript": "^5.7.2",
    "ts-morph": "^24.0.0",
    "@babel/parser": "^7.26.3",
    "@typescript-eslint/typescript-estree": "^8.18.0",
    "acorn": "^8.14.0",
    "acorn-typescript": "^1.4.13"
  }
}
