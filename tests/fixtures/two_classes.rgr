; Test: Two classes - minimal ChessPiece and ChessBoard interaction

class ChessPiece {
    def symbol:string ""
    
    Constructor (s:string) {
        symbol = s
    }
    
    sfn Empty:ChessPiece () {
        return (new ChessPiece(" "))
    }
}

class ChessBoard {
    def board:[ChessPiece]
    
    Constructor () {
        this.init()
    }
    
    fn init:void () {
        push board (ChessPiece.Empty())
    }
}

class TwoClassesTest {
    sfn m@(main):void () {
        def b (new ChessBoard())
        print "Done"
    }
}
