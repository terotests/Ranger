{
  "Class Definition": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName} {",
      "\tdef ${2:property}:${3:string} \"\"",
      "\t",
      "\tConstructor (${4:param}:${5:string}) {",
      "\t\t${2:property} = ${4:param}",
      "\t}",
      "\t",
      "\tfn ${6:method}:${7:void} () {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Create a new class"
  },
  "Main Entry Point": {
    "prefix": "main",
    "body": [
      "class ${1:Main} {",
      "\tsfn m@(main):void () {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Create main entry point"
  },
  "Function": {
    "prefix": "fn",
    "body": ["fn ${1:name}:${2:void} (${3:param}:${4:string}) {", "\t$0", "}"],
    "description": "Create an instance function"
  },
  "Static Function": {
    "prefix": "sfn",
    "body": ["sfn ${1:name}:${2:void} (${3:param}:${4:string}) {", "\t$0", "}"],
    "description": "Create a static function"
  },
  "Constructor": {
    "prefix": "ctor",
    "body": ["Constructor (${1:param}:${2:string}) {", "\t$0", "}"],
    "description": "Create a constructor"
  },
  "If Statement": {
    "prefix": "if",
    "body": ["if (${1:condition}) {", "\t$0", "}"],
    "description": "If statement"
  },
  "If-Else Statement": {
    "prefix": "ife",
    "body": ["if (${1:condition}) {", "\t$2", "} {", "\t$0", "}"],
    "description": "If-else statement"
  },
  "While Loop": {
    "prefix": "while",
    "body": ["while (${1:condition}) {", "\t$0", "}"],
    "description": "While loop"
  },
  "For Loop": {
    "prefix": "for",
    "body": ["for ${1:list} ${2:item}:${3:string} ${4:i} {", "\t$0", "}"],
    "description": "For loop over array"
  },
  "Switch Statement": {
    "prefix": "switch",
    "body": [
      "switch ${1:value} {",
      "\tcase ${2:val1} {",
      "\t\t$3",
      "\t}",
      "\tdefault {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Switch statement"
  },
  "Try-Catch": {
    "prefix": "try",
    "body": ["try {", "\t$1", "} {", "\t$0", "}"],
    "description": "Try-catch block"
  },
  "Define Variable": {
    "prefix": "def",
    "body": ["def ${1:name}:${2:string} ${3:value}"],
    "description": "Define a variable"
  },
  "Define Array": {
    "prefix": "defarr",
    "body": ["def ${1:name}:[${2:string}]"],
    "description": "Define an array"
  },
  "Define Map": {
    "prefix": "defmap",
    "body": ["def ${1:name}:[${2:string}:${3:int}]"],
    "description": "Define a map/hash"
  },
  "Define Optional": {
    "prefix": "defopt",
    "body": ["def ${1:name}@(optional):${2:string}"],
    "description": "Define an optional variable"
  },
  "Define Mutable": {
    "prefix": "defmut",
    "body": ["def ${1:name}@(mutable):${2:int} ${3:0}"],
    "description": "Define a mutable variable"
  },
  "Lambda Function": {
    "prefix": "lambda",
    "body": ["(fn:${1:void} (${2:param}:${3:string}) {", "\t$0", "})"],
    "description": "Create a lambda function"
  },
  "New Instance": {
    "prefix": "new",
    "body": ["(new ${1:ClassName}(${2:args}))"],
    "description": "Create new instance"
  },
  "Import": {
    "prefix": "import",
    "body": ["Import \"${1:filename}.clj\""],
    "description": "Import a file"
  },
  "Enum": {
    "prefix": "enum",
    "body": ["Enum ${1:EnumName} ( ${2:Value1} ${3:Value2} )"],
    "description": "Define an enum"
  },
  "Extends": {
    "prefix": "extends",
    "body": ["Extends(${1:ParentClass})"],
    "description": "Extend a parent class"
  },
  "Print": {
    "prefix": "print",
    "body": ["print \"${1:message}\""],
    "description": "Print to console"
  },
  "Optional Check": {
    "prefix": "ifnull",
    "body": [
      "if (!null? ${1:optionalVar}) {",
      "\tdef ${2:value} (unwrap ${1:optionalVar})",
      "\t$0",
      "}"
    ],
    "description": "Check and unwrap optional"
  },
  "Array Push": {
    "prefix": "push",
    "body": ["push ${1:array} ${2:item}"],
    "description": "Push item to array"
  },
  "Array Iteration": {
    "prefix": "foreach",
    "body": ["for ${1:array} ${2:item}:${3:string} ${4:idx} {", "\t$0", "}"],
    "description": "Iterate over array"
  }
}
