{
  "name": "ranger-compiler",
  "version": "3.0.0-alpha.1",
  "description": "Ranger Compiler NPM environment",
  "main": "dist/bin/api.js",
  "types": "dist/bin/api.d.ts",
  "bin": {
    "ranger-compiler": "bin/output.js",
    "rgrc": "bin/output.js"
  },
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "test-any-ts": "node bin/testcomp.js ./features/any/test_any.rgr -es6 -typescript",
    "test-any-csharp": "node bin/testcomp.js ./features/any/test_any.rgr -l=csharp",
    "test-any": "npm run testcompiler && node bin/testcomp.js -es6 ./features/any/test_any.rgr -es6 -o=test_any.js",
    "example-svgpath": "rgrc -es6 ./examples/SVGPath/SVGPath.rgr -es6 -typescript -o=SVGPath.ts",
    "testcompiler": "rgrc -es6 ./compiler/ng_Compiler.rgr -nodecli -d=./bin -o=testcomp.js",
    "compileg": "rgrc -es6 ./compiler/ng_Compiler.rgr -nodecli",
    "compile": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -es6 ./compiler/ng_Compiler.rgr -nodecli -d=./bin -o=output.js",
    "compile:rust": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -l=rust ./compiler/ng_Compiler.rgr -nodecli -d=./bin -o=compiler",
    "compile:langserver": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -es6 -nodemodule ./compiler/VirtualCompiler.rgr -d=./ranger-vscode-extension/compiler -o=output.js",
    "try": "node bin/output.js -es6 ./compiler/ng_Compiler.rgr -nodecli -o=try.js",
    "compilets": "node bin/output.js -es6 -typescript ./compiler/ng_Compiler.rgr -o=compiler.ts -nodecli && tsc",
    "module": "node bin/output.js -nodemodule -es6 -typescript ./compiler/ng_Compiler.rgr -o=api.ts && tsc",
    "test": "vitest run --config tests/vitest.config.ts",
    "test:es6": "vitest run --config tests/vitest.config.ts compiler.test.ts",
    "test:go": "vitest run --config tests/vitest.config.ts compiler-go.test.ts",
    "test:python": "vitest run --config tests/vitest.config.ts compiler-python.test.ts",
    "test:rust": "vitest run --config tests/vitest.config.ts compiler-rust.test.ts",
    "test:kotlin": "vitest run --config tests/vitest.config.ts compiler-kotlin.test.ts",
    "test:watch": "vitest --config tests/vitest.config.ts",
    "gen:simple": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -es6 ./generated/SimpleChessBoard.rgr -d=./generated/output -o=SimpleChessBoard.js -nodecli",
    "gen:simple:run": "npm run gen:simple && node ./generated/output/SimpleChessBoard.js",
    "gen:chess": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -es6 ./generated/ChessBoard.rgr -d=./generated/output -o=ChessBoard.js -nodecli",
    "gen:chess:run": "npm run gen:chess && node ./generated/output/ChessBoard.js",
    "gen:chess:rust": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -l=rust ./generated/ChessBoard.rgr -d=./generated/bin -o=chessboard.rs -nodecli",
    "gen:chess:rust:build": "npm run gen:chess:rust && rustc ./generated/bin/chessboard.rs -o ./generated/bin/chessboard.exe",
    "gen:chess:rust:run": "npm run gen:chess:rust:build && ./generated/bin/chessboard.exe",
    "gen:chess:swift": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -l=swift3 ./generated/ChessBoard.rgr -d=./generated/bin -o=chessboard.swift -nodecli",
    "gen:all": "npm run gen:simple && npm run gen:chess",
    "gen:test": "npm run gen:simple:run",
    "game:compile": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -es6 ./gallery/invaders/invaders.rgr -d=./gallery/invaders -o=invaders.js -nodecli",
    "game:compile:rust": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -l=rust ./gallery/invaders/invaders.rgr -d=./gallery/invaders -o=invaders.rs -nodecli",
    "game:compile:go": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -l=go ./gallery/invaders/invaders.rgr -d=./gallery/invaders -o=invaders.go -nodecli",
    "game:compile:swift": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -l=swift6 ./gallery/invaders/invaders.rgr -d=./gallery/invaders -o=invaders.swift -nodecli",
    "game:compile:cpp": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -l=cpp ./gallery/invaders/invaders.rgr -d=./gallery/invaders -o=invaders.cpp -nodecli",
    "game:compile:kotlin": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -l=kotlin ./gallery/invaders/invaders.rgr -d=./gallery/invaders -o=invaders.kt -nodecli",
    "game:compile:python": "cross-env RANGER_LIB=./compiler/Lang.rgr;./lib/stdops.rgr node bin/output.js -l=python ./gallery/invaders/invaders.rgr -d=./gallery/invaders -o=invaders.py -nodecli",
    "game:build:rust": "npm run game:compile:rust && rustc gallery/invaders/invaders.rs -o gallery/invaders/invaders_rust.exe",
    "game:build:go": "npm run game:compile:go && cd gallery/invaders && go build -o invaders_go.exe invaders.go",
    "game:build:swift": "npm run game:compile:swift && swiftc -parse-as-library gallery/invaders/invaders.swift -o gallery/invaders/invaders_swift.exe",
    "game:build:cpp": "npm run game:compile:cpp && g++ -std=c++17 -pthread gallery/invaders/invaders.cpp -o gallery/invaders/invaders_cpp.exe",
    "game:build:kotlin": "npm run game:compile:kotlin && kotlinc gallery/invaders/invaders.kt -include-runtime -d gallery/invaders/invaders.jar",
    "game:build:all": "npm run game:compile && npm run game:build:rust && npm run game:build:go",
    "game:run": "node ./gallery/invaders/invaders.js",
    "game:run:rust": "./gallery/invaders/invaders_rust.exe",
    "game:run:go": "./gallery/invaders/invaders_go.exe",
    "game:run:swift": "./gallery/invaders/invaders_swift.exe",
    "game:run:cpp": "./gallery/invaders/invaders_cpp.exe",
    "game:run:kotlin": "java -jar ./gallery/invaders/invaders.jar",
    "game": "npm run game:compile && npm run game:run",
    "jsparser:compile": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -es6 ./gallery/js_parser/js_parser_main.rgr -d=./gallery/js_parser -o=js_parser.js -nodecli",
    "jsparser:compile:cpp": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -l=cpp ./gallery/js_parser/js_parser_main.rgr -d=./gallery/js_parser -o=js_parser -nodecli",
    "jsparser:compile:swift": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -l=swift6 ./gallery/js_parser/js_parser_main.rgr -d=./gallery/js_parser -o=js_parser -nodecli",
    "jsparser:compile:go": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -l=go ./gallery/js_parser/js_parser_main.rgr -d=./gallery/js_parser -o=js_parser.js -nodecli",
    "jsparser:module": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -es6 -nodemodule ./gallery/js_parser/js_parser_main.rgr -d=./gallery/js_parser/benchmark -o=js_parser_module.cjs",
    "jsparser:compile:rust": "cross-env RANGER_LIB=./compiler/Lang.rgr node bin/output.js -l=rust ./gallery/js_parser/js_parser_main.rgr -d=./gallery/js_parser -o=js_parser -nodecli",
    "jsparser:build:rust": "npm run jsparser:compile:rust && rustc -O gallery/js_parser/js_parser.rs -o gallery/js_parser/js_parser_rust.exe",
    "jsparser:build:cpp": "wsl x86_64-w64-mingw32-g++-posix -std=c++17 -static -O3 gallery/js_parser/js_parser.cpp -o gallery/js_parser/js_parser_cpp.exe",
    "jsparser:build:swift": "swiftc -O -parse-as-library gallery/js_parser/js_parser.swift -o gallery/js_parser/js_parser_swift.exe",
    "jsparser:compile:all": "npm run jsparser:compile:cpp && npm run jsparser:compile:rust && npm run jsparser:compile:swift",
    "jsparser:build:all": "npm run jsparser:compile:all && npm run jsparser:build:cpp && npm run jsparser:build:rust && npm run jsparser:build:swift",
    "jsparser:run": "node ./gallery/js_parser/js_parser.js",
    "jsparser:run:rust": "./gallery/js_parser/js_parser_rust",
    "jsparser:test": "node ./gallery/js_parser/js_parser.js ./gallery/js_parser/test.js",
    "jsparser": "npm run jsparser:compile && npm run jsparser:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terotests/Ranger.git"
  },
  "keywords": [
    "Compiler"
  ],
  "author": "Tero Tolonen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/terotests/Ranger/issues"
  },
  "homepage": "https://github.com/terotests/Ranger#readme",
  "devDependencies": {
    "@types/node": "^22.19.2",
    "cross-env": "^10.1.0",
    "vitest": "^1.6.0"
  }
}
